<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:id="@+id/nav_photo_detail"
    app:startDestination="@id/fragment_photo_detail">

    <fragment
        android:id="@+id/fragment_photo_detail"
        android:name="com.example.lookpin.feature.photodetail.PhotoDetailFragment"
        android:label="@string/feature_photo_detail">

        <argument
            android:name="photoId"
            app:argType="string" />
    </fragment>

    <action
        android:id="@+id/action_global_photo_detail"
        app:destination="@id/nav_photo_detail">
        <argument
            android:name="photoId"
            app:argType="string" />
    </action>

</navigation>
